{
    "name": "Visual Story Points for Jira",
    "description": "Relative estimates can be fun!",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://issues.redhat.com/*"],
          "css": ["style.css"],
          "js": ["content-script.js"]
        }
    ],
      "icons": {
        "128": "icon-128.png",
        "32": "icon-32.png",
        "16": "icon-16.png"
      }
  }