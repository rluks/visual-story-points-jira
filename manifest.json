{
    "name": "Visual Story Points for Jira",
    "description": "Relative estimates can be fun!",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["style.css"],
          "js": [
            "inject.js",
            "content.js"
          ]
        }
    ],
    "icons": {
        "128": "icon-128.png",
        "32": "icon-32.png",
        "16": "icon-16.png"
    },
    "web_accessible_resources": [
      {
        "resources": [ "img/*.png"],
        "matches": [ "<all_urls>" ]
      }
    ]
  }